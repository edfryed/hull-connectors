{
  "name": "hull",
  "version": "0.14.0-beta.14",
  "description": "A Node.js client for hull.io",
  "main": "src",
  "repository": {
    "type": "git",
    "url": "https://github.com/hull/hull-node.git"
  },
  "homepage": "https://github.com/hull/hull-node",
  "bugs": {
    "url": "https://github.com/hull/hull-node/issues"
  },
  "engines": {
    "node": ">=6.12.0"
  },
  "author": "Romain Dardour <romain@hull.io>",
  "license": "MIT",
  "scripts": {
    "test": "npm run test:lint && npm run test:flow && npm run test:unit && npm run test:integration",
    "test:lint": "eslint src && documentation lint src",
    "test:unit": "NODE_ENV=test mocha --require babel-register -R spec ./test/unit/*.js ./test/unit/**/*.js",
    "test:integration": "NODE_ENV=test mocha --require babel-register -R spec ./test/integration/*.js",
    "documentation": "documentation build src/**/* src/types.js -f md -o API.md --access public --markdownToc=false",
    "precommit": "npm run documentation && git add API.md"
  },
  "dependencies": {
    "aws-sdk": "^2.81.0",
    "batch-stream": "^0.1.3",
    "bull": "^3.0.0-rc.3",
    "connect": "^3.4.1",
    "connect-timeout": "^1.8.0",
    "csv-stream": "^0.1.3",
    "del": "^2.2.1",
    "dogapi": "^2.6.0",
    "jsonwebtoken": "^7.4.3",
    "jwt-simple": "^0.5.0",
    "kue-ui": "^0.1.0",
    "passport": "^0.3.2",
    "progress-bar-webpack-plugin": "^1.11.0",
    "promise-streams": "^1.0.1",
    "promisepipe": "^2.1.3",
    "raw-body": "^2.1.7",
    "react-hot-loader": "^4.2.0",
    "request": "^2.72.0",
    "sns-validator": "^0.3.0",
    "sqs-consumer": "^3.6.1",
    "supply": "0.0.4",
    "webpack": "^3.12.0",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.22.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "chai-http": "^3.0.0",
    "husky": "^0.14.3",
    "isparta": "^4.0.0",
    "mkdirp": "^0.5.1",
    "node-mocks-http": "^1.7.0",
    "nyc": "^11.0.3",
    "supertest": "^3.1.0",
    "updtr": "^1.0.0"
  },
  "nodeBoilerplateOptions": {
    "mochaGlobals": [
      "stub",
      "spy",
      "expect"
    ]
  },
  "nyc": {
    "include": [
      "src"
    ]
  }
}
