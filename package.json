{
  "name": "hull-connectors",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "duplicated-dependencies": "syncpack-list-mismatches -s 'packages/*/package.json' -s 'packages/connectors/*/package.json'",
    "build": "babel packages -d dist --delete-dir-on-start && rsync -r packages/ dist/ --ignore-existing",
    "start:dev": "sh scripts/start-dev-connector.sh",
    "test:lint": "eslint packages"
  },
  "private": true,
  "workspaces": [
    "packages/connectors/*",
    "packages/hull",
    "packages/hull-client"
  ],
  "engines": {
    "node": "8.12.x",
    "npm": "6.4.x",
    "yarn": "1.9.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "documentation": "^8.1.2",
    "eslint": "^5.6.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.0",
    "flow-bin": "^0.82.0",
    "flow-typed": "^2.5.1",
    "minihull": "^3.0.2",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "syncpack": "^3.0.0"
  },
  "dependencies": {
    "JSONStream": "^1.3.4",
    "basic-auth": "^2.0.1",
    "bluebird": "3",
    "body-parser": "^1.18.3",
    "cache-manager": "^2.9.0",
    "chai": "^4.2.0",
    "datadog-metrics": "^0.8.1",
    "debug": "^4.0.1",
    "ejs": "^2.6.1",
    "express": "4",
    "kue": "^0.11.6",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "newrelic": "^4.9.0",
    "nock": "^10.0.0",
    "raven": "^2.6.4",
    "sinon": "^6.3.4",
    "sinon-chai": "^3.2.0",
    "superagent": "3",
    "urijs": "^1.19.1",
    "uuid": "^3.3.2"
  }
